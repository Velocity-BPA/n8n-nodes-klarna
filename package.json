{
  "name": "n8n-nodes-klarna",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Klarna Buy Now Pay Later providing 9 resources and 35+ operations for payment processing, order management, and settlements. Includes webhook triggers and multi-region support.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-klarna.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-klarna",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "klarna",
    "bnpl",
    "buy-now-pay-later",
    "payments",
    "checkout",
    "installments",
    "fintech",
    "ecommerce"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/KlarnaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Klarna/Klarna.node.js",
      "dist/nodes/Klarna/KlarnaTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "gulp": "^4.0.2",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
